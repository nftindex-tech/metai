{
    "name": "greensea/keccak",
    "description": "Pure PHP implementation of Keccak. Fork from kornrunner/php-keccak, support PHP 7.0",
    "keywords": ["keccak", "sha-3"],
    "license": "MIT",
    "authors": [
        {
            "name": "greensea",
            "homepage": "https://github.com/greensea/php-keccak"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "kornrunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kornrunner\\": "test"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "satooshi/php-coveralls": "~2"
    }
}
